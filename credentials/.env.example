# Runtime Environment
RUNTIME_ENVIRONMENT=development
PORT_NUMBER=8500

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com

# Google Cloud Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLOUD_PROJECT=your_project_id
CREDENTIALS_PATH=/path/to/your/credentials.json

# Milvus Configuration
MILVUS_USERNAME=root
MILVUS_PASSWORD=milvus
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_DB=default

# Langfuse Configuration
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=https://cloud.langfuse.com

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/dbname

# Bot Configuration
BOT_API_URL=your_bot_api_url
WEBHOOK_SECRET=your_webhook_secret
OUTLINE_API_KEY=your_outline_api_key

# Default Messages
DEFAULT_ANSWER=Xin lỗi, tôi không thể trả lời câu hỏi này. Vui lòng thử lại sau.
OVERLOAD_MESSAGE=Hệ thống đang quá tải, vui lòng thử lại sau.
CS_MESSAGE=Vui lòng liên hệ với bộ phận hỗ trợ khách hàng.
NO_RELEVANT_GETFLY_MESSAGE=Không tìm thấy thông tin liên quan.

# TensorFlow Configuration
TF_ENABLE_ONEDNN_OPTS=1
TF_CPP_MIN_LOG_LEVEL=2 